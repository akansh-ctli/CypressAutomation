{
    "name": "@centurylink/po-ng911",
    "version": "0.0.3",
    "main": "dist/@centurylink/po-ng911.common.js",
    "types": "index.d.ts",
    "files": [
        "dist/**/*",
        "index.d.ts"
    ],
    "private": false,
    "scripts": {
        "serve": "vue-cli-service serve --mode dev",
        "build": "vue-cli-service build --mode dev",
        "build:pr": "vue-cli-service build --mode pr",
        "build:stage": "vue-cli-service build --mode stage",
        "build:library": "bash tools/library-create-package.sh && vue-cli-service build --mode prod --target lib src/build/microapp-library.ts --formats commonjs",
        "build:component": "bash tools/clean.sh && vue-cli-service build --mode prod --target lib src/build/component-library.ts --formats commonjs --name @centurylink/po-ng911-component",
        "pack:library": "bash tools/pre-publish.sh 'library' && npm pack && bash tools/post-publish.sh 'library'",
        "pack:component": "bash tools/pre-publish.sh 'component' && npm pack && bash tools/post-publish.sh 'component'",
        "docker:build": "docker build --build-arg GH_TOKEN -t microapp .",
        "docker:start": "docker run -it --rm --cap-add=SYS_ADMIN --name microapp-run -v ${PWD}:/portal-microapp-sample -p 9090:9090 microapp bash ./docker-run.sh",
        "lint": "eslint '{src,tests}/**/*.{js,ts,vue}'",
        "prettier:checker": "prettier './{src/**/*(*.ts|*.vue),*.js,tests/**/*(*.js)}' --check",
        "prettier": "prettier './{src/**/*(*.ts|*.vue),*.js,tests/**/*(*.js)}' --write",
        "test:e2e:local": "vue-cli-service test:e2e --mode dev --env VUE_APP_SHELL=http://localhost:5000/",
        "test:e2e:dev": "vue-cli-service test:e2e --mode dev",
        "test:e2e": "vue-cli-service test:e2e --mode stage",
        "test:e2e:jenkins": "vue-cli-service test:e2e --headless --mode stage",
        "test:unit": "vue-cli-service test:unit"
    },
    "dependencies": {},
