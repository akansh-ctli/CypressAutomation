        "@types/d3": "^5.7.2",
        "@types/googlemaps": "^3.37.5",
        "@types/lodash.compact": "^3.0.6",
        "@types/lodash.find": "^4.6.6",
        "@types/lodash.first": "^3.0.6",
        "@types/lodash.flatten": "^4.4.6",
        "@types/lodash.keys": "^4.2.6",
